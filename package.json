{
  "name": "lapelo",
  "version": "0.0.1",
  "decription": "一个简单的随机图片服务端，随机返回一张程序所在目录下的图片，常用于在线随机壁纸服务。",
  "license": "MIT",
  "scripts": {
    "run": "node index.js",
    "pkg": "pkg . --compress GZip --output lapelo.exe"
  },
  "dependencies": {
    "compression": "1.7.4",
    "express": "4.18.2",
    "select-random-file": "1.2.4",
    "winston": "3.8.2",
    "yaml": "2.1.3"
  },
  "devDependencies": {
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-node": "11.1.0"
  },
  "repository": {
    "type": "git",
    "url": "//github.com/Giftia/Lapelo.git"
  },
  "bin": "index.js",
  "pkg": {
    "assets": [],
    "targets": [
      "node14-win-x64"
    ],
    "outputPath": "."
  },
  "engines": {
    "node": "~14"
  }
}
